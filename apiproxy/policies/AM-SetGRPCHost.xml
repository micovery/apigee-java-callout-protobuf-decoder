<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="AM-SetGRPCHost">
    <DisplayName>AM-SetGRPCHost</DisplayName>
    <Properties/>
    <Set>
        <Headers>
            <Header name="X-Host-Port">{request.header.host}</Header>
        </Headers>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>