<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RegularExpressionProtection continueOnError="false" enabled="true" name="RegexTP-CheckBody">
    <DisplayName>RegexTP-CheckBody</DisplayName>
    <Properties/>
    <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
    <Variable name="decoded-message">
        <Pattern>\bNL[0-9]{2}[A-Z]{4}[0-9]{10}\b</Pattern>
    </Variable>
    <Source>request</Source>
</RegularExpressionProtection>